<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.WizardWorld" name="WizardWorld" version="1.0" provider-name="Worldkodi">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">WizardWorld</summary>
    <description lang="en">WizardWorld allows you to get access to the latest WizardWorld Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>